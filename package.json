{
  "name": "@hasna/connectors",
  "version": "0.0.5",
  "description": "Open source connector library - Install API connectors with a single command",
  "type": "module",
  "bin": {
    "connectors": "./bin/index.js"
  },
  "files": [
    "bin/",
    "dist/",
    "connectors/",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/cli/index.tsx --outdir ./bin --target bun --external ink --external react --external chalk --external conf && bun build ./src/index.ts --outdir ./dist --target bun",
    "dev": "bun run ./src/cli/index.tsx",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "connectors",
    "api",
    "cli",
    "typescript",
    "bun",
    "figma",
    "stripe",
    "github",
    "openai"
  ],
  "author": "Hasna",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.2.0",
    "typescript": "^5"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.2.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasna/opensource-connectors.git"
  }
}
