{
  "name": "@hasna/connect-uspto",
  "version": "0.0.1",
  "description": "USPTO API connector CLI - Patent and trademark search with browser automation",
  "type": "module",
  "bin": {
    "connect-uspto": "./bin/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --external playwright --external playwright-core && bun build ./src/cli/index.ts --outdir ./bin --target bun --external playwright --external playwright-core",
    "dev": "bun run ./src/cli/index.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "uspto",
    "patent",
    "trademark",
    "intellectual-property",
    "api",
    "connector",
    "cli",
    "typescript",
    "bun",
    "playwright",
    "hasna"
  ],
  "author": "Hasna",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "playwright": "^1.41.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasna/opensource-connectors.git"
  }
}
