{
  "name": "@hasna/connect-sedo",
  "version": "0.1.0",
  "description": "Sedo Domain Marketplace API connector CLI - Search, list, and manage domains on Sedo",
  "author": "Hasna",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "connect-sedo": "./bin/index.js"
  },
  "files": [
    "dist",
    "bin",
    "src"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bun build ./src/cli/index.ts --outdir ./bin --target bun",
    "dev": "bun run --watch src/cli/index.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build",
    "release": "bun run ./scripts/publish.ts",
    "release:patch": "bun run ./scripts/publish.ts patch",
    "release:minor": "bun run ./scripts/publish.ts minor",
    "release:major": "bun run ./scripts/publish.ts major",
    "release:dry": "bun run ./scripts/publish.ts --dry-run"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "fast-xml-parser": "^4.5.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "sedo",
    "domain",
    "marketplace",
    "api",
    "connector",
    "cli",
    "typescript",
    "bun",
    "hasna"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasna/connectors.git"
  },
  "private": true
}