{
  "name": "@hasna/connect-twilio",
  "version": "0.1.0",
  "description": "Twilio API connector CLI - A TypeScript wrapper for the Twilio API",
  "type": "module",
  "bin": {
    "connect-twilio": "bin/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bun build ./src/cli/index.ts --outdir ./bin --target bun",
    "dev": "bun run ./src/cli/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build",
    "release": "bun run ./scripts/publish.ts",
    "release:patch": "bun run ./scripts/publish.ts patch",
    "release:minor": "bun run ./scripts/publish.ts minor",
    "release:major": "bun run ./scripts/publish.ts major",
    "release:dry": "bun run ./scripts/publish.ts --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasna/connectors.git"
  },
  "homepage": "https://github.com/hasna/connectors#readme",
  "bugs": {
    "url": "https://github.com/hasna/connectors/issues"
  },
  "keywords": [
    "twilio",
    "api",
    "connector",
    "cli",
    "typescript",
    "bun",
    "sms",
    "voice",
    "video",
    "messaging",
    "verify",
    "hasna"
  ],
  "author": "Hasna",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ]
}