{
  "name": "@hasna/connect-discord",
  "version": "0.1.0",
  "description": "Discord API connector CLI - Guilds, Channels, Messages, Webhooks, Commands, and more",
  "type": "module",
  "bin": {
    "connect-discord": "./bin/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bun build ./src/cli/index.ts --outdir ./bin --target bun",
    "dev": "bun run ./src/cli/index.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "discord",
    "discord-api",
    "discord-bot",
    "connector",
    "cli",
    "typescript",
    "bun",
    "guilds",
    "channels",
    "messages",
    "webhooks",
    "slash-commands",
    "hasna"
  ],
  "author": "Hasna",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hasna/connectors.git"
  }
}