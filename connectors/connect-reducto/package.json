{
  "name": "@hasna/connect-reducto",
  "version": "0.1.0",
  "description": "Reducto API connector - Document parsing, extraction, and manipulation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "connect-reducto": "./bin/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "bun run src/cli/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target node && bun build ./src/cli/index.ts --outdir ./dist/cli --target node && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "reducto",
    "document",
    "parsing",
    "pdf",
    "extraction",
    "ocr",
    "api",
    "hasna"
  ],
  "author": "Hasna",
  "license": "MIT"
}
